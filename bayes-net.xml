<?xml version="1.0"?>
<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
	      <!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
	      <!ATTLIST VARIABLE TYPE (nature|decision|utility) "nature">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>


<BIF VERSION="0.3">
<NETWORK>
<NAME>bayes-net-config</NAME>
<VARIABLE TYPE="nature">
	<NAME>lag2</NAME>
</VARIABLE>
<VARIABLE TYPE="nature">
	<NAME>lag1</NAME>
</VARIABLE>
<VARIABLE TYPE="nature">
	<NAME>class</NAME>
</VARIABLE>
<DEFINITION>
    <FOR>lag2</FOR>
    <TABLE></TABLE>
</DEFINITION>
<DEFINITION>
    <FOR>lag1</FOR>
    <GIVEN>lag2</GIVEN>
    <TABLE></TABLE>
</DEFINITION>
<DEFINITION>
    <FOR>class</FOR>
    <GIVEN>lag1</GIVEN>
    <TABLE></TABLE>
</DEFINITION>
</NETWORK>
</BIF>
